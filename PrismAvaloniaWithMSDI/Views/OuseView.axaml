             xmlns:prism="http://prismlibrary.com/"
             xmlns:viewmodels="using:PrismAvaloniaWithMSDI.ViewModels"
             prism:ViewModelLocator.AutoWireViewModel="True"
             x:Class="PrismAvaloniaWithMSDI.Views.OuseView"
